{
  "name": "api-mint",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "nodemon -r dotenv/config ./dist/server",
    "start": "node ./dist/server",
    "docker:build": "yarn build && docker build -t garciadiazjaime/api-mint .",
    "docker:run": "docker run -d -p 49330:3030 --name events garciadiazjaime/api-mint",
    "docker:push": "docker push garciadiazjaime/api-mint",
    "docker:pull": "docker pull garciadiazjaime/api-mint"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "convict": "^4.0.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "mint-api-events": "^1.0.1",
    "mint-api-news": "^1.0.20",
    "mongoose": "^5.0.0-rc2",
    "morgan": "^1.9.0",
    "redis": "^2.8.0",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.14.7"
  }
}
