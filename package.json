    "body-parser": "^1.18.2",
    "convict": "^4.0.2",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "express": "^4.16.2",
    "mint-api-events": "^1.0.1",
    "mint-api-news": "^1.0.20",
    "mongoose": "^5.0.0-rc2",
    "morgan": "^1.9.0",
    "redis": "^2.8.0",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "nodemon": "^1.14.7"
  }
}
